    </xs:complexType>
    <xs:complexType name="FallbackStarType">
        <xs:sequence>
            <xs:group maxOccurs="unbounded" minOccurs="1" ref="BuiltInMultipleTypes"/>
        </xs:sequence>
        <xs:attribute name="name" type="xs:string" use="optional"/>
    </xs:complexType>
    <xs:complexType name="ParallelType">
        <xs:sequence>
            <xs:group maxOccurs="unbounded" minOccurs="2" ref="BuiltInMultipleTypes"/>
        </xs:sequence>
        <xs:attribute name="name" type="xs:string" use="optional"/>
        <xs:attribute name="threshold" type="xs:string" use="required"/>
    </xs:complexType>
    <xs:complexType name="RetryType">
        <xs:group ref="BuiltInMultipleTypes"/>
        <xs:attribute name="name" type="xs:string" use="optional"/>

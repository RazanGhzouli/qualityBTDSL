<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!--Created with Liquid Studio 2018 (https://www.liquid-technologies.com)--><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
    <xs:element name="root">
        <xs:complexType>
            <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="1" name="BehaviorTree">
                    <xs:complexType>
                        <xs:group ref="BuiltInMultipleTypes"/>
                        <xs:attribute name="ID" type="xs:string" use="required"/>
                    </xs:complexType>
                </xs:element>
                <xs:element maxOccurs="1" minOccurs="0" name="TreeNodeModel">
